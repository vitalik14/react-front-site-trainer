{
  "manifest_version": 2,
  "name": "pur-life",
  "version": "5.2",
  "default_locale": "de",
  "description": "pur-life - das Erste Live-Online-Fitnessstudio",
  "display": "standalone",
  "orientation": "portrait-primary",
  "theme_color": "#3bafda",
  "background_color": "#293033",
  "prefer_related_applications": true,
  "related_applications": [
    {
      "platform": "play",
      "id": "de.purlife.app"
    }
  ],
  "icons": [
    {
      "src": "images/launcher/launcher-icon-0-75x.png",
      "sizes": "36x36",
      "type": "image/png",
      "density": 0.75
    },
    {
      "src": "images/launcher/launcher-icon-1x.png",
      "sizes": "48x48",
      "type": "image/png",
      "density": 1.0
    },
    {
      "src": "images/launcher/launcher-icon-1-5x.png",
      "sizes": "72x72",
      "type": "image/png",
      "density": 1.5
    },
    {
      "src": "images/launcher/launcher-icon-2x.png",
      "sizes": "96x96",
      "type": "image/png",
      "density": 2.0
    },
    {
      "src": "images/launcher/launcher-icon-3x.png",
      "sizes": "144x144",
      "type": "image/png",
      "density": 3.0
    },
    {
      "src": "images/launcher/launcher-icon-4x.png",
      "sizes": "192x192",
      "type": "image/png",
      "density": 4.0
    },
	{
	  "src": "images/launcher/512x512.png",
	  "sizes": "512x512",
	  "type": "image/png"
	}
  ]
}